	</servlet>

	<servlet-mapping>
		<servlet-name>firstServlet</servlet-name>
		<url-pattern>/FirstServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>DemoServ</servlet-name>
		<url-pattern>/DemoServ</url-pattern>
	</servlet-mapping>
	

</web-app>